{
  "name": "bitespeed-identity-service",
  "version": "1.0.0",
  "description": "Identity reconciliation service",
  "main": "dist/index.js",
  "engines": {
    "node": "20.11.1"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "rm -rf dist && tsc --project tsconfig.json && cp -r src/*.json dist/",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "start:prod": "node dist/src/index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "typeorm": "^0.3.20",
    "reflect-metadata": "^0.2.1",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
} 